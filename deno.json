{
  "name": "@overlay-stack/tooltip",
  "version": "0.0.1",
  "license": "MIT",
  "exports": "./index.ts",
  "tasks": {
    "dev": "deno run -A npm:vite",
    "build": "deno run -A npm:vite build && deno run --allow-read --allow-write build.ts"
  },
  "nodeModulesDir": "auto",
  "compilerOptions": {
    "types": [
      "react",
      "react-dom",
      "@types/react"
    ],
    "lib": [
      "dom",
      "dom.iterable",
      "deno.ns"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "lint": {
    "rules": {
      "include": ["no-console", "no-debugger", "no-slow-types"]
    },
    "exclude": ["build.ts"]
  },
  "imports": {
    "react": "npm:react@^19.2.0",
    "react-dom": "npm:react-dom@^19.2.0",
    "react-dom/client": "npm:react-dom@^19.2.0/client",
    "react/jsx-runtime": "npm:react@^19.2.0/jsx-runtime",
    "react/jsx-dev-runtime": "npm:react@^19.2.0/jsx-dev-runtime",
    "vite": "npm:vite@^7.2.4",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^5.1.1",
    "@types/react": "npm:@types/react@^19.2.6",
    "@types/react-dom": "npm:@types/react-dom@^19.2.3",
    "typescript": "npm:typescript@^5.3.3",
    "path": "https://deno.land/std@0.211.0/path/mod.ts",
    "tsc": "npm:typescript@^5.3.3/bin/tsc"
  },
  "ts": {
    "moduleFileExtensions": [
      ".ts",
      ".tsx",
      ".jsx",
      ".json",
      ".css",
      ".module.css"
    ]
  }
}
